var AUTHORS=''

use import="packages\GitTagVersion.Console.0.1.0\tools\sake\gittagversion.shade"
@{
	var versionInfo=GitTagVersion();
	var VERSION = versionInfo["ShortVersion"];
	var FULL_VERSION = versionInfo["ShortSemVersion"];
	var FULL_SEMVERSION = versionInfo["FullSemVersion"];
}
-//log info="	VERSION: ${VERSION}"
-//log info="	FULL_VERSION: ${FULL_VERSION}"
log info="  semversion: ${FULL_SEMVERSION}"

use-standard-lifecycle
use-nsemversion-goals features="xunit2,nuget"
